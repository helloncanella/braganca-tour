{
  "eslintIntegration": false,
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "parser": "babylon",
  "semi": false,
  "useTabs": false,
  "javascriptEnable": [
    "javascript",
    "javascriptreact"
  ],
  "typescriptEnable": [
    "typescript",
    "typescriptreact"
  ],
  "cssEnable": [
    "css",
    "less",
    "scss"
  ],
  "jsonEnable": [
    "json"
  ],
  "graphqlEnable": [
    "graphql"
  ],
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "parser": "markdown",
        "semi": true
      }
    }
  ]
}